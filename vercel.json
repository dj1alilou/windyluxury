{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "env": {
    "API_BASE": "https://windy-luxury-2.onrender.com/api"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://windy-luxury-2.onrender.com/api/$1"
    },
    {
      "src": "/ping",
      "dest": "https://windy-luxury-2.onrender.com/ping"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "https://windy-luxury-2.onrender.com/uploads/$1"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/(.*\\.js)$",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/(.*\\.css)$",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/(.*\\.(jpg|jpeg|png|gif|svg|ico))$",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "env": {
    "API_BASE": "https://windy-luxury-2.onrender.com/api"
  },
  "routes": [
    {
      "src": "^/ping/?$",
      "dest": "https://windy-luxury-2.onrender.com/ping"
    },
    {
      "src": "^/admin/?$",
      "dest": "/admin.html"
    },
    {
      "src": "^/uploads/(.*)$",
      "dest": "https://windy-luxury-2.onrender.com/uploads/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://windy-luxury-2.onrender.com/api/$1"
    },
    {
      "src": "/(.*)/",
      "dest": "/$1/index.html"
    },
    {
      "src": "/(.*[^\\..*])/?$",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

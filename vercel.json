{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "cleanUrls": false,
  "trailingSlash": false,
  "routes": [
    {
      "src": "^/admin/?$",
      "dest": "/admin.html"
    },
    {
      "src": "^/uploads/(.*)$",
      "dest": "https://windy-luxury-2.onrender.com/uploads/$1",
      "headers": {
        "x-vercel-proxy": "render-backend"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "https://windy-luxury-2.onrender.com/api/$1",
      "headers": {
        "x-vercel-proxy": "render-backend"
      }
    },
    {
      "src": "/(.*)/",
      "dest": "/$1/index.html"
    },
    {
      "src": "/(.*[^\\..*])/?$",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
